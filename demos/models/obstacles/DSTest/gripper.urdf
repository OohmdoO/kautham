    <inertial>
	<mass value="1"/>
	<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>
  
<joint name="joint4" type="prismatic">
