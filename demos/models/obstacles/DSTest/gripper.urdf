<?xml version="1.0"?>
<robot name="gripper">
   
<link name="gripper">
    <visual>
      <geometry>
	<box size="0.002 0.006 0.002" />
      </geometry>
      <origin rpy="0 0 1.57" xyz="0 0 0001"/>
      <material name="white">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
	<box size="0.002 0.006 0.002" />
      </geometry>
      <origin rpy="0 0 1.57" xyz="0 0 0001"/>
    </collision>
    <inertial>
	<mass value="1"/>
	<inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>



<link name="finger1">
    <visual>
      <geometry>
	<box size="0.002 0.002 0.006" />
      </geometry>
      <origin rpy="0 0 1.57" xyz="0 0 0001"/>
      <material name="white">
        <color rgba="0.5 0.5 0.5 1"/>
