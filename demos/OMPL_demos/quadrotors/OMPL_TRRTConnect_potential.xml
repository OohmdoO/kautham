<?xml version="1.0"?>
<Problem name="OMPL_RRTStar_bigcube">
	<Robot robot="robots/bigcube.dh" scale="40">
		<Limits name="X" min="0" max="100" />
		<Limits name="Y" min="0" max="100" />
		<Home TH="0" WZ="1" WY="0" WX="0" Z="0" Y="0" X="0" />
	</Robot>
	<Obstacle obstacle="obstacles/potential.iv" scale="1">
		<Home TH="0" WZ="1" WY="0" WX="0" Z="0" Y="0" X="0" />
		<Collision enabled="false"/>
	</Obstacle>
	<Controls robot="controls/bigcube_R2.cntr" />
	<Planner>
		<Parameters>
			<Name>omplTRRTConnect</Name>
			<Potential potential="controls/potential.xml"/>
