			<Parameter name="Speed Factor">1</Parameter>
			<Parameter name="Range">2</Parameter>
			<Parameter name="Simplify Solution">1</Parameter>
			<Parameter name="Max States Failed">20</Parameter>
			<Parameter name="Max States Succeed">0</Parameter>
			<Parameter name="Init Temp">0.05</Parameter>
		</Parameters>
		<Queries>
			<Query>
				<Init dim="2">0.1 0.1</Init>
				<Goal dim="2">0.9 0.9</Goal>
			</Query>
		</Queries>
	</Planner>
</Problem>
